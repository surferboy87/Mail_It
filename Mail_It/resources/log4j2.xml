<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="info">
  <Appenders>
    <Console name="CONSOLE_OUT" target="SYSTEM_OUT">
      <PatternLayout pattern="### %-4p ###   %d{dd.MM.yyyy HH:mm:ss} %C{2}:  %m%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="info">
      <AppenderRef ref="CONSOLE_OUT"/>
    </Root>
  </Loggers>
</Configuration>
